#!/bin/bash

if [ $# -gt 0 ]
then
    if [ $? -eq 0 ]; then
        REGIONS=$@
        for REGION in $REGIONS; do
            aws ec2 describe-vpcs --region "$REGION" | jq '.Vpcs[].VpcId'
        done
    else
        echo "incorrect command"
    fi
else
    echo "Please provide the correct arguments"
fi
